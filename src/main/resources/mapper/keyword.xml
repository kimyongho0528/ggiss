<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="api.autocomplete.infrastructure.mybatis.KeywordMapper">

    <resultMap id="keywordMap" type="api.autocomplete.infrastructure.dto.KeywordDTO">
        <result property="keyword"			column="keyword"/>
    </resultMap>

    <select id="findKeywords" resultMap="keywordMap">
        select keyword from KEYWORD
    </select>
</mapper>
